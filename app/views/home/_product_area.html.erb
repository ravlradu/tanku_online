<!--product area start-->
<div class="product_area  mb-64">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="product_header">
                    <div class="section_title">
                       <p>Recently added our store</p>
                       <h2>Trending Products</h2>
                    </div>
                    <div class="product_tab_btn">
                        <ul class="nav" role="tablist">
                            <li>
                                <% @main_categories.shuffle[0..3].each do |category| %>
                                  <a class="active" data-toggle="tab" href="#plant2" role="tab" aria-controls="plant1" aria-selected="true">
                                      <%= category.name %>
                                  </a>
                                <% end %>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="product_container">
           <div class="row">
               <div class="col-12">
                    <div class="tab-content">
                        <div class="tab-pane fade show active" id="plant1" role="tabpanel">
                            <div class="product_carousel product_column5 owl-carousel">

                                  <% 0.upto(@products.length/2) do |column| %>
                                    <div class="product_items">
                                        <% @products[column*2..column*2+1].each do |product| %>
                                          <%= render partial: "product", locals:{product:product} %>
                                        <% end %>
                                    </div>
                                  <% end %>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
